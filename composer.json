{
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/videlalvaro/php-amqplib.git"
        },
        {
            "type": "package",
            "package": {
                "name": "videlalvaro/php-amqplib",
				"version": "dev-master",
				"dist": {
                    "url": "https://github.com/videlalvaro/php-amqplib/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/videlalvaro/php-amqplib",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    
    "require": {
        "php": ">=5.3.2",
		"symfony/framework-standard-edition": "v2.1.0",
		"symfony/symfony": "v2.1.0",
        "twig/twig": "v1.9.2",
        "propel/propel-bundle": "1.1.3",
        "propel/propel1": "1.6.7",
        "willdurand/propel-typehintable-behavior": "1.0.3",
        "knplabs/knp-menu-bundle": "v1.1.0",
        "white-october/pagerfanta-bundle": "dev-master",
        "friendsofsymfony/jsrouting-bundle": "1.0.1",		
        "friendsofsymfony/user-bundle": "dev-master",
        "hwi/oauth-bundle": "*",
        "snc/redis-bundle": "dev-master",
        "oldsound/rabbitmq-bundle": "dev-master",
        "videlalvaro/php-amqplib": "dev-master",
        "behat/behat": "2.3.5",        
        "behat/behat-bundle": "dev-master",
		"stfalcon/tinymce-bundle": "dev-master",
		"CarpeHora/ExtraPropertiesBehavior": "dev-master",
		"imagine/Imagine": "dev-master",
		"knplabs/gaufrette": "dev-master",
		"knplabs/knp-gaufrette-bundle": "dev-master",
		"amazonwebservices/aws-sdk-for-php": "dev-master",
		"vich/geographical-bundle": "dev-master",
		"knplabs/knp-snappy-bundle": "dev-master",
		"predis/predis": "0.7.*" 
    },
	
	"minimum-stability": "dev",
	
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ]
    },
	
    "config": {
        "bin-dir": "bin"
    },
	
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    },
	
    "autoload": {
        "psr-0": {
            "BNS\\App": "src/",
            "BNS\\Central": "src/",
			"BNS\\Vendor": "src/"
        },
		"classmap": ["vendor/propel/propel1/generator/lib/util/PropelPHPParser.php"]
    }
}