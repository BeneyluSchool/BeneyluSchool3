{
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/videlalvaro/php-amqplib.git"
        },
        {
            "type": "package",
            "package": {
                "name": "videlalvaro/php-amqplib",
                "version": "dev-master",
                "dist": {
                    "url": "https://github.com/videlalvaro/php-amqplib/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/videlalvaro/php-amqplib",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],

    "require": {
        "php": ">=5.3.9",
        "symfony/framework-standard-edition": "2.7.2",
        "symfony/symfony": "2.7.2",
        "monolog/monolog": "1.9.1",
        "twig/twig": "v1.18.2",
        "propel/propel-bundle": "1.1.7",
        "propel/propel1": "1.6.x-dev#7b8597359b1048bdd7d2e2afa4731d17b01a1716",
        "phing/phing": "2.4.12",
        "willdurand/propel-typehintable-behavior": "1.0.3",
        "knplabs/knp-menu-bundle": "v1.1.0",
        "white-october/pagerfanta-bundle": "dev-master",
        "friendsofsymfony/jsrouting-bundle": "dev-master#481f24ec727ecd47409e36fd18739b44bac78b15",
        "friendsofsymfony/oauth-server-bundle": "dev-master",
        "friendsofsymfony/rest-bundle": "0.8.0",
        "friendsofsymfony/user-bundle": "dev-master",
        "hwi/oauth-bundle": "*",
        "sensio/buzz-bundle": "dev-master",
        "snc/redis-bundle": "dev-master",
        "oldsound/rabbitmq-bundle": "dev-master",
        "videlalvaro/php-amqplib": "dev-master",
        "behat/behat": "2.3.5",
        "behat/behat-bundle": "dev-master",
        "stfalcon/tinymce-bundle": "dev-master#915a9029238ea5b201f9505e6d4d7c52362de1eb",
        "CarpeHora/ExtraPropertiesBehavior": "dev-master",
        "imagine/Imagine": "dev-master",
        "knplabs/gaufrette": "dev-master",
        "knplabs/knp-gaufrette-bundle": "dev-master",
        "amazonwebservices/aws-sdk-for-php": "dev-master",
        "vich/geographical-bundle": "dev-master",
        "knplabs/knp-snappy-bundle": "dev-master",
        "predis/predis": "0.7.*",
        "exercise/htmlpurifier-bundle": "dev-master",
        "opensky/runtime-config-bundle": "dev-master",
        "elao/web-profiler-extra-bundle": "2.1.x-dev"
    },

    "minimum-stability": "dev",

    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ]
    },

    "config": {
        "bin-dir": "bin"
    },

    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    },

    "autoload": {
        "psr-0": {
            "BNS\\App": "src/",
            "BNS\\Central": "src/",
            "BNS\\Vendor": "src/"
        },
        "classmap": [
            "vendor/propel/propel1/generator/lib/util/PropelPHPParser.php",
            "src/BNS/App/CoreBundle/Utils/vendor/simple_html_dom.php",
            "src/BNS/App/CoreBundle/Utils/vendor/rss_php.php",
            "src/BNS/App/CoreBundle/Utils/vendor/videopian.php",
            "src/BNS/App/FixtureBundle/Vendor/Spyc/Spyc.php"
        ],
        "files": [
            "src/BNS/App/CoreBundle/Utils/vendor/url_to_absolute/url_to_absolute.php"
        ]
    }
}
