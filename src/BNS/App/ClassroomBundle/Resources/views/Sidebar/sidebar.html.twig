{% if page == 'classroom-visualisation' %}
    <div class="container-sidebar list">
        <div class="section full-name active">
            <div class="section-title">
                <h1 class="dashboard"><img src="{{ avatar(user, 'thumbnail') }}" /> {{ user.getFullName() }}</h1>

            </div>
            <span class="shadow-section"></span>
        </div>
        {% if not isEnabled %}
            <div class="title-container">
                <div class="content-title">
                    <ul>
                        <li class="text-row">
                            <div>
                                <div class="bns-alert alert alert-danger">
                                    {% trans %}THIS_PUPIL_IS_DESACTIVATE{% endtrans %}
                                </div>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>


        {% endif %}
        <div class="title-container">
            <div class="title border-blue active">{% trans %}GENERAL_INFORMATION{% endtrans %} <span class="square icon-arrow"></span></div>
            <div class="content-title" style="display: block;">
                <div class="content-profile">
                    <p class="type">{{ 'LASTNAME_USER'|trans({'%user%': user.getLastName()|escape}) |desc('<strong>Nom :</strong> %user%')|raw }}</p>
                    <p class="type">{{ 'FIRSTNAME_USER'|trans({'%user%': user.getFirstName()|escape}) |desc('<strong>Pr√©nom :</strong> %user%')|raw }}</p>
                    <p class="type">{{ 'LOGIN_USER'|trans({'%user%': user.getLogin()|escape}) |desc('<strong>Identifiant :</strong> %user%')|raw }}</p>

                    {% if user.getBirthday() != null %}
                        <p class="type">
                            {% if user.getGender() == 'F' %}
                                {{ 'BORN_USER'|transchoice(1, {'%date%': user.getBirthday()|date_bns('long', 'none') }) |desc('{0} <strong> N√© le : </strong>%date% | {1} <strong> N√©e le : </strong%date%>') |raw }}
                            {% else %}
                                {{ 'BORN_USER'|transchoice(0, {'%date%': user.getBirthday()|date_bns('long', 'none') }) |desc('{0} <strong> N√© le : </strong>%date% | {1} <strong> N√©e le : </strong%date%>') |raw }}
                            {% endif %}
                        </p>
                    {% endif %}
                </div>

              {% if is_child(user) %}
                <div class="content-profile">
                  <p class="type"><strong>{% trans %}PARENT_ASSOCIATE{% endtrans %} : </strong></p>
                  {% if parents|length > 0 %}
                    <div class="block-users parents">
                      {% for parent in parents %}
                        <a class="bns-user small" href="{{ path('BNSAppClassroomBundle_back_pupil_detail_edit', {'userSlug': parent.slug}) }}">
                          <img src="{{ avatar(parent, 'thumbnail') }}" alt=""/>
                          <span class="name">{{ parent.getFullName() }}</span>
                        </a>
                      {% endfor %}
                    </div>

                  {% else %}
                    <i>{% trans %}NO_PARENT_ASSOCIATE{% endtrans %}</i>

                  {% endif %}
                </div>
                <div class="content-profile">
                  {% if assistants|length > 0 %}
                    <p class="type"><strong>{% trans %}ASSISTANT_ASSOCIATE{% endtrans %} : </strong></p>
                    <div class="block-users parents">
                      {% for assistant in assistants %}
                        <a class="bns-user small" href="{{ path('BNSAppClassroomBundle_back_pupil_detail_edit', {'userSlug': assistant.slug}) }}">
                          <img src="{{ avatar(assistant, 'thumbnail') }}" alt=""/>
                          <span class="name">{{ assistant.getFullName() }}</span>
                        </a>
                      {% endfor %}
                    </div>
                  {% endif %}
                </div>
                <br/>
              {% endif %}
            </div>
        </div>
    </div>
{% endif %}

{% if page == 'classroom-user-edit' %}
    <div class="container-sidebar list editor">
        <div class="section full-name active">
            <div class="section-title">
                <h1 class="dashboard"><img src="{{ avatar(user, 'thumbnail') }}" /> {{ user.getFullName() }}</h1>

            </div>
            <span class="shadow-section"></span>
        </div>
        <div class="title-container">
            <div class="title border-blue active">{% trans %}GENERAL_INFORMATION{% endtrans %} <span class="square icon-arrow"></span></div>
            <div class="content-title" style="display: block;">
                <div class="content-profile">
                    <input type="hidden" id="default-avatar" value="{{ avatar(user, 'small', true) }}"/>
                    <div id="resource-selection-callback">
                        <img src="{{ avatar(user, 'small') }}" />
                    </div>
                    {% if user.getId() == app.user.getId() %}
                        {% set button_label = 'CHOOSE_MY_AVATAR'|trans %}
                        {% set button_delete_label = 'DELETE_MY_AVATAR'|trans %}


                    {% else %}
                        {% set button_label = "CHANGE_MY_AVATAR"|trans %}
                        {% set button_delete_label = "DELETE_AVATAR"|trans %}


                    {% endif %}
                    {% include 'BNSAppMediaLibraryBundle:Front:selection.html.twig' with {
                    'label' : button_label,
                    'input_id': 'profile_form_avatarId',
                    'allowed_type': 'IMAGE',
                    'name': form.avatarId.vars.full_name,
                    'callback': 'resource-selection-callback',
                    'with_cancel': true,
                    'current_src': avatar(user, 'small'),
                    'default_src': avatar(user, 'small',true),
                    'current_value': form.avatarId.vars.value
                    } %}

                    <p class="type">
                        <strong>{% trans %}LABEL_NAME{% endtrans %}</strong>

                    </p>
                    {{ form_widget(form.lastName) }}
                    {% if form.lastName.vars.errors|length > 0 %}
                        <div class="alert alert-danger bns-alert">
                            {{ form_errors(form.lastName) }}
                        </div>
                    {% endif %}
                    <p class="type">
                        <strong>{% trans %}LABEL_FIRSTNAME{% endtrans %} </strong>

                    </p>
                    {{ form_widget(form.firstName) }}
                    {% if form.firstName.vars.errors|length > 0 %}
                        <div class="alert alert-danger bns-alert">
                            {{ form_errors(form.firstName) }}
                        </div>
                    {% endif %}

                    {% if is_adult(user) %}
                        <p class="type"><strong>{% trans %}LABEL_EMAIL{% endtrans %} </strong></p>

                        {{ form_widget(form.email) }}
                        {% if form.email.vars.errors|length > 0 %}
                            <div class="alert alert-danger bns-alert">
                                {{ form_errors(form.email) }}
                            </div>
                        {% endif %}
                    <p class="type"><strong>{% trans %}LABEL_CHANGE_LANGUAGE{% endtrans %}</strong></p>
                        {{ form_widget(form.lang) }}
                    {% endif %}
                    <p class="type">
                        {% if user.getGender() == 'F' %}
                            {{ 'LABEL_BORN_USER'|transchoice(1, {'%date%': user.getBirthday}) |desc('{0} <strong> N√© le : </strong> | {1} <strong> N√©e le : </strong>') |raw }}
                        {% else %}
                            {{ 'LABEL_BORN_USER'|transchoice(0, {'%date%': user.getBirthday}) |desc('{0} <strong> N√© le : </strong> | {1} <strong> N√©e le : </strong>') |raw }}
                        {% endif %}
                    </p>
                    <div class="birthday-datepicker">
                        {{ form_widget(form.birthday) }}
                    </div>
                    {% if form.birthday.vars.errors|length > 0 %}
                        <div class="alert alert-danger bns-alert">
                            {{ form_errors(form.birthday) }}
                        </div>
                    {% endif %}
                    <p class="type">
                        <strong>{{ 'LABEL_GENDER'|trans  |desc('Sexe :') }} </strong>
                    </p>
                    <div  class="gender">
                        {{ form_widget(form.gender) }}
                    </div>
                    {% if form.gender.vars.errors|length > 0 %}
                        <div class="alert alert-danger bns-alert">
                            {{ form_errors(form.gender) }}
                        </div>
                    {% endif %}
                </div>
                {% if is_child(user) %}
                    <div class="content-profile">
                        <p class="type"><strong>{% trans %}PARENT_ASSOCIATE{% endtrans %} : </strong></p>
                        {% if parents|length > 0 %}
                            <div class="block-users parents">
                                {% for parent in parents %}
                                    <a href="#" class="bns-user small checkbox selected" data-user-id="{{ parent.getId() }}">
                                        <img src="{{ avatar(parent, 'thumbnail') }}" alt="" />
                                        <span class="name">{{ parent.getFullName() }}</span>
                                        <span class="action-icons"></span>
                                    </a>
                                {% endfor %}
                            </div>
                        {% else %}
                            <i>{% trans %}NO_PARENT_ASSOCIATE{% endtrans %}</i>

                        {% endif %}
                    </div>
                  {% if assistants|length > 0 %}
                    <div class="content-profile">
                      <p class="type"><strong>{% trans %}ASSISTANT_ASSOCIATE{% endtrans %} : </strong></p>
                      <div class="block-users assistants">
                        {% for assistant in assistants %}
                          <a href="#" class="bns-user small checkbox selected" data-user-id="{{ assistant.id }}">
                            <img src="{{ avatar(assistant, 'thumbnail') }}" alt="" />
                            <span class="name">{{ assistant.getFullName() }}</span>
                            <span class="action-icons"></span>
                          </a>
                        {% endfor %}
                      </div>
                  </div>
                  {% endif %}
                  <br/>
                {% endif %}
            </div>
        </div>
    </div>
{% endif %}

