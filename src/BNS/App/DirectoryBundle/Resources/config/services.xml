<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="install_process_bns_app_directory_bundle.class">BNS\App\DirectoryBundle\Install\InstallProcess</parameter>
    </parameters>

    <services>
        <service id="install_process_bns_app_directory_bundle" class="%install_process_bns_app_directory_bundle.class%">
            <tag name="bns.install_process" />
        </service>
    </services>
</container>
