{% trans_default_domain "LIAISONBOOK" %}
        <bns-expander label="{{ 'TITLE_ARCHIVED' | trans }}" class="ng-scope ng-isolate-scope" is-open="true">
            <md-radio-group ng-init="liaisonbook.date='{{ app.session.get('liaisonbook-archive-date-month')~ '_' ~ app.session.get('liaisonbook-archive-date-year') }}'" ng-model="liaisonbook.date">
                {% for dateArchive in datesArchives %}
                <md-radio-button value="{{ dateArchive|date("n")}}_{{ dateArchive|date("Y") }}" id="{{ dateArchive|date("n")}}_{{ dateArchive|date("Y") }}" class="md-primary archive_link">{{dateArchive|date_bns('none', 'none', 'MMMM y')}}</md-radio-button>
                {% endfor %}
            </md-radio-group>
        </bns-expander>
