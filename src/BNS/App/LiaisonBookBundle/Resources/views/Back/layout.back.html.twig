{% extends 'BNSAppMainBundle:App:base.html.twig' %}

{% set app_unique_name = 'LIAISONBOOK' %}
{% set app_mode = 'back' %}
{% trans_default_domain "LIAISONBOOK" %}
{% block title %}{{ 'TITLE_BENEYLU_LIAISON_MANAGEMENT'|trans({'%beneylu_brand_name%': parameter('beneylu_brand_name')}) }}{% endblock %}
{% block body_class %}{{ parent() }} module-liaison module-back back{% endblock %}
{% block body_attr %} data-app="{{ app_unique_name }}" data-mode="{{ app_mode }}" {{ parent() }} {% endblock %}
{% block javascripts %}
    {{ parent() }}
    <script type="text/javascript" src="{{ asset('/medias/js/liaisonbook/Back/layout.js') }}"></script>
{% endblock %}
{% block content %}
    <md-content class="bns-content-inner layout-column">
        {{ block('content') }}
    </md-content>
{% endblock %}
