{% extends 'BNSAppMainBundle:App:base.html.twig' %}

{% set app_unique_name = 'MINISITE' %}
{% set app_mode = 'back' %}
{% trans_default_domain "MINISITE" %}
{% block body_class %}{{ parent() }}module-minisite module-back back{% endblock %}
{% block body_attr %}{{ parent() }} data-app="{{ app_unique_name }}" data-mode="{{ app_mode }}" {% endblock %}
{% block css %}
  {{ parent() }}
  <link href="{{ asset('/medias/css/minisite/manager.css') }}" rel="stylesheet" type="text/css"/>
  <link href='https://fonts.googleapis.com/css?family=Lobster' rel='stylesheet' type='text/css'>
{% endblock css %}
{% block javascripts %}
  {{ parent() }}

  <script type="text/javascript" src="{{ asset('/medias/js/bootstrap/bootstrap-tooltip.js') }}"></script>
  <script type="text/javascript" src="{{ asset('/medias/js/bootstrap/bootstrap-popover.js') }}"></script>
  <script type="text/javascript" src="{{ asset('/medias/js/jquery.ui.nestedSortable.js') }}"></script>
  <script type="text/javascript">
    $(function ()
    {
      if ($('.alert-flash').length > 0) {
        setTimeout(function ()
        {
          $('.alert-flash div').slideUp('fast', function () { $(this).parent().slideUp('fast', function () { $('.bns-alert div').show() }) });
        }, 8000);
      }
    });
  </script>
{% endblock %}




{% block actionbar_content %}
  {% block header %}
  {% endblock %}
{% endblock %}


{% block content %}
  {{ block('content') }}
{% endblock %}


