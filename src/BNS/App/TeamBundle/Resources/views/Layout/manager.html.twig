{% set app_unique_name = 'TEAM' %}
{% set app_mode = 'back' %}

{% block title %}
	{% trans %}TITLE_TEAM_ADMIN{% endtrans %}
{% endblock %}

{% block javascripts %}
    {{ parent() }}

    <script type="text/javascript" src="{{ asset('/medias/js/team/team.js') }}"></script>
{% endblock %}

{% block css %}
	{{ parent() }}

	<link href="{{ asset('/medias/css/team/manager.css') }}" rel="stylesheet" type="text/css" />
