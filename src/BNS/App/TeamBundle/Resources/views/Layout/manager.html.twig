{% endblock %}

{% block javascripts %}
    {{ parent() }}
    
    <script type="text/javascript" src="{{ asset('/medias/js/team/team.js') }}"></script>
{% endblock %}

{% block css %}
	{{ parent() }}
	
	<link href="{{ asset('/medias/css/team/manager.css') }}" rel="stylesheet" type="text/css" />
{% endblock %}

{% block body %}
	<body>
		<div class="office blur">  </div>
        <div class="container">
            <div class="row-fluid">
                {% block content %}{% endblock %}
            </div>
        </div>
	</body>
{% endblock %}
	
{% block dock_bar %}
	{% render "BNSAppMainBundle:DockBar:index" with {'module_unique_name' : 'TEAM' , 'is_in_front' : false } %}
{% endblock %}