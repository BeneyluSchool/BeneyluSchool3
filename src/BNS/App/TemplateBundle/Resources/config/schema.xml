<?xml version="1.0" encoding="UTF-8"?><database defaultIdMethod="native" name="app" defaultPhpNamingMethod="phpname" namespace="BNS\App\TemplateBundle\Model">  <table name="template">    <column autoIncrement="true" name="id" primaryKey="true" required="true" type="INTEGER" />    <column name="label" required="true" size="255" type="VARCHAR" />    <column name="css_class" required="true" size="255" type="VARCHAR" />    <column name="template_type_id" required="true" type="INTEGER" />    <foreign-key foreignTable="template_type">      <reference foreign="id" local="template_type_id" />    </foreign-key>    <behavior name="i18n">      <parameter name="i18n_columns" value="label" />      <parameter name="locale_column" value="lang" />      <parameter name="i18n_table" value="template_i18n" />      <parameter name="i18n_phpname" value="TemplateI18n" />    </behavior>  </table>  <table name="template_type">    <column autoIncrement="true" name="id" primaryKey="true" required="true" type="INTEGER" />    <column name="bundle_name" required="true" size="255" type="VARCHAR" />  </table>  <table name="template_entity_collection">    <column name="template_id" primaryKey="true" required="true" type="INTEGER" />    <column name="template_entity_id" primaryKey="true" required="true" type="INTEGER" />    <foreign-key foreignTable="template">      <reference foreign="id" local="template_id" />    </foreign-key>    <foreign-key foreignTable="template_entity">      <reference foreign="id" local="template_entity_id" />    </foreign-key>  </table>  <table name="template_entity">    <column autoIncrement="true" name="id" primaryKey="true" required="true" type="INTEGER" />    <column name="label" required="true" size="255" type="VARCHAR" />    <column name="css_class" required="true" size="255" type="VARCHAR" />    <column name="type" required="true" type="ENUM" valueSet="COLOR, FONT, BACKGROUND" />	<column name="data" type="array" />    <behavior n